{
	"AppSettings": {
		"Host": "<Host Server Name or Ip>",
		"Domain": "",
		"SharedAuthTicketKeyPath": "/home/session",
		"SharedAuthTicketKeyCertificateFilePath": "<SSL 인증서 파일 경로>",
		"SharedAuthTicketKeyCertificatePassword": "<SSL 인증서 비밀번호>",
		"ExpireMinutes": 1440,
		"CreateAccountWhenNotExist": true,
		"DatabaseType":"<MariaDB/MongoDB>",
		"RabbitMq": {
			"Name": "PortalSvr",
			"Host": "localhost",
			"Port": 5672,
			"VirtualHost": "/",
			"User": "<RabbitMq 아이디>",
			"Password": "<RabbitMq 비밀번호>",
			"ExchangeName": "ksan.system",
			"Enabled": true
		},
		"AllowedHosts": "*"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"PortalProvider": "Information"
		}
	},
	"MariaDB":{
		"Host": "localhost",
		"Name": "ksan",
		"Port":3306,
		"User":"<DB 접속유저>",
		"Password": "<DB 비밀번호>",
		"LicenseKey":""
	},
	"MongoDB": {
		"Host": "localhost",
		"Name": "ksan",
		"Port":3306,
		"User":"<DB 접속유저>",
		"Password": "<DB 비밀번호>"
	}
}
